    "version": "0.2.0",
    "configurations": [
        {
            "name": "sgx-gdb Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../build/linux-x86_64-normal-server-release/jdk/bin/java",
            "args": ["TestSuit"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/../ev_test/",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}